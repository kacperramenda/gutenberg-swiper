{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gutenberg-swiper/container",
  "version": "1.0.0",
  "title": "Swiper Slider",
  "category": "media",
  "icon": "images-alt2",
  "description": "Kontener dla slajd√≥w.",
  "textdomain": "gutenberg-swiper",
  "attributes": {
    "autoplay": { "type": "boolean", "default": true },
    "autoplayDelay": { "type": "number", "default": 3000 },
    "speed": { "type": "number", "default": 500 },
    "effect": { "type": "string", "default": "slide" },
    "loop": { "type": "boolean", "default": true }
  },
  "editorScript": "gutenberg-slider-editor-script",
  "style": "gutenberg-slider-style",
  "viewScript": "file:../../build/view.js"
}